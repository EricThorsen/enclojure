                    </file>
                    <file name="org-openide-actions-ToolsAction.shadow">
                        <attr name="originalFile" stringvalue="Actions/System/org-openide-actions-ToolsAction.instance"/>
                        <attr name="position" intvalue="1300"/>
                    </file>
                    <file name="sep-1.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="200"/>
                    </file>
                    <file name="sep-2.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="500"/>
                    </file>
                    <file name="sep-3.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="800"/>
                    </file>
                    <file name="sep-4.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1000"/>
                    </file>
                    <file name="sep-5.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1200"/>
                    </file>
                </folder>
                <folder name="Factories">
                    <file name="ClojureDataLoader.instance">
                        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/enclojure/editor/Clojure 16x16.png"/>
                        <attr name="dataObjectClass" stringvalue="org.enclojure.editor.ClojureDataObject"/>
                        <attr name="instanceCreate" methodvalue="org.openide.loaders.DataLoaderPool.factory"/>
                        <attr name="mimeType" stringvalue="text/x-clojure"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="ClojureResolver.xml" url="ClojureResolver.xml">
                <attr name="displayName" bundlevalue="org.enclojure.editor.Bundle#Services/MIMEResolver/ClojureResolver.xml"/>
            </file>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="Other">
            <file name="ClojureTemplate.clj" url="ClojureTemplate.clj">
                <attr name="displayName" bundlevalue="org.enclojure.editor.Bundle#Templates/Other/ClojureTemplate.clj"/>
                <attr name="template" boolvalue="true"/>
            </file>
        </folder>
    </folder>
</filesystem>
